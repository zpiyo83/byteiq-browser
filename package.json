{
  "name": "byteiq-browser",
  "version": "0.1.2",
  "description": "A community-driven AI browser powered by Chromium kernel",
  "main": "src/main/main.js",
  "build": {
    "win": {
      "target": "nsis",
      "signAndEditExecutable": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "browser",
    "ai",
    "chromium",
    "electron",
    "community-driven"
  ],
  "author": "Byteiq Community",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^26.7.0"
  },
  "dependencies": {
    "electron-store": "^8.1.0"
  }
}
